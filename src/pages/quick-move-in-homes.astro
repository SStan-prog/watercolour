---
import Layout from '../layouts/PageLayout.astro';
import Header from '../components/QuickMoveInHomes/Header.astro';
import SitePlan from '../components/QuickMoveInHomes/SitePlan.astro';
import BuyersJourney from '../components/QuickMoveInHomes/BuyersJourney.astro';
import Bungalows from '../components/QuickMoveInHomes/Cards/Bungalows.astro';
import Sold from '../components/QuickMoveInHomes/Cards/Sold.astro';

import { quickMoveInHomesPageQuery } from '../queries/pages/quick-move-in-moves';
import { bungalowsQuery } from '../queries/mrhomes/bungalows';
import { soldHomesQuery } from '../queries/mrhomes/sold';
const data = await quickMoveInHomesPageQuery();
const bungalows = await bungalowsQuery();
const soldHomes = await soldHomesQuery();

const seo = data.page.seo;
const header = data.page.quick_move_in_homes.header;
const sitePlan = data.page.quick_move_in_homes.sitePlan;
const buyersJourney = data.page.quick_move_in_homes.homeBuyersJourney;
---

<Layout seo={seo}>
  <Header header={header} />
  <SitePlan sitePlan={sitePlan} />
  <BuyersJourney buyersJourney={buyersJourney} />
  <Bungalows bungalows={bungalows} />
  <Sold soldHomes={soldHomes} />
</Layout>
