---
import Layout from "../layouts/PageLayout.astro";
import Header from "../components/Common/Header/header.astro";
import Main from "../components/Faq/main.astro";

import { faqPageQuery } from "../queries/api";
const data = await faqPageQuery();
const header = data.page.faq.header;
const main = data.page.faq.main;
---

<Layout title="Welcome to Astro.">
  <Header header={header} />
  <Main main={main} />
</Layout>
